<AttachmentInfo>
	<!--
		name:         Name of the attachment as stored on the file system.
		displayName:  Name of the attachment as entered by the user.
		size:         Size of the attachment as a string for display..
		bytes:        Size of the attachment in bytes.
		convert:      True if the attachment is converted from on filetype to another.
		extract:      True if the attachment is extracted from a MimeMessage.
	-->
    	<Attachment 
		name="db__diagram.png" 
		displayName="db#$diagram.png" 
		size="25KB" 
		bytes="30000" 
		convert="1" 
		extract="1" 
	/>
</AttachmentInfo>
