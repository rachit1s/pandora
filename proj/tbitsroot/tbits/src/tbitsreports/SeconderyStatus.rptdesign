            <property name="queryText">select r.request_id,r.due_datetime,r.subject,t.display_name status,tc.display_name category
from requests r
join types t on t.sys_id  = r.sys_id and r.status_id = t.type_id and t.field_id  = 4
join types tc on tc.sys_id  = r.sys_id and r.category_id = tc.type_id and tc.field_id  = 3
where r.sys_id  = 15 and t.display_name = ?</property>
